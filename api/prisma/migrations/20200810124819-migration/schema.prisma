datasource DS {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Category {
    id          Int     @id @default(autoincrement())
    name        String
    items       Item[]
}

model Image {
    id          Int     @id @default(autoincrement())
    title       String
    url         String
}

model Item {
    id          Int     @id @default(autoincrement())
    name        String
    category    Category @relation( fields: [categoryId], references: [id] )
    categoryId  Int
    createAt    DateTime @default(now())
}
